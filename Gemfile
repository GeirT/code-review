# A sample Gemfile
source "https://rubygems.org"

gem "sinatra"
gem "pony"
gem "sentry-raven"

group :development do
  gem "rack-test"

  gem "rake"
  gem "guard"
  gem "guard-minitest"
  gem "rb-inotify", require: false
  gem "rb-fsevent", require: false
  gem "rb-fchange", require: false
end

group :test do
  gem "mocha"
end
